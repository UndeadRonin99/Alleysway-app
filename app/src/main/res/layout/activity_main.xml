<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_app_login_page"
    android:layout_width="match_parent"
    android:layout_height="844dp"
    android:background="@drawable/container_app_login_page">

    <ImageView
        android:id="@+id/image_login"
        android:layout_width="match_parent"
        android:layout_height="933dp"
        android:layout_gravity="top|left"
        android:scaleType="fitStart"
        android:src="@drawable/image_login" />

    <ImageView
        android:id="@+id/image_rectangle"
        android:layout_width="345dp"
        android:layout_height="488dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="26dp"
        android:layout_gravity="center"
        android:background="@drawable/image_rectangle"
        android:scaleType="centerInside" />

    <ImageView
        android:id="@+id/image_logo"
        android:layout_width="123dp"
        android:layout_height="68dp"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="19dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_logo" />

    <FrameLayout
        android:id="@+id/container_group"
        android:layout_width="283dp"
        android:layout_height="149dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="393dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_group">

        <FrameLayout
            android:id="@+id/container_group1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/container_group1">

            <FrameLayout
                android:id="@+id/container_pass"
                android:layout_width="73dp"
                android:layout_height="41dp"
                android:layout_marginLeft="20dp"
                android:layout_gravity="top|left">

                <TextView
                    android:id="@+id/text_username"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_gravity="top"
                    android:layout_marginTop="-1dp"
                    android:layout_marginBottom="-1dp"
                    android:gravity="center_horizontal|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Username"
                    android:textColor="#FFFFFF"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/text_username_input"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="23dp"
                    android:layout_gravity="left"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="aman|"
                    android:textColor="#FFEAEAEA"
                    android:textSize="14dp" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/container_email"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:background="@drawable/container_email">

                <TextView
                    android:id="@+id/text_password"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_gravity="left"
                    android:layout_marginLeft="22dp"
                    android:layout_marginTop="84dp"
                    android:layout_marginBottom="35dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Password"
                    android:textColor="#FFFFFF"
                    android:textSize="15dp"
                    android:translationY="-10dp" />

                <ImageView
                    android:id="@+id/image_rectangle2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="94dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/image_rectangle1"
                    android:translationY="-10dp" />

                <ImageView
                    android:id="@+id/image_rectangle1"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_gravity="top"
                    android:scaleType="centerCrop"
                    android:src="@drawable/image_rectangle1" />
            </FrameLayout>
        </FrameLayout>
    </FrameLayout>

    <TextView
        android:id="@+id/container_group2"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_marginLeft="115dp"
        android:layout_marginTop="324dp"
        android:layout_marginBottom="-5dp"
        android:layout_gravity="top|left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Welcome Back"
        android:textColor="#FFFFFFFF"
        android:textSize="22dp" />

    <ImageView
        android:id="@+id/image_rectangle3"
        android:layout_width="290dp"
        android:layout_height="55dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="618dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_rectangle3"
        android:translationY="-25dp" />

    <FrameLayout
        android:id="@+id/container_button"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="619dp"
        android:layout_marginRight="70dp"
        android:layout_marginBottom="175dp"
        android:translationY="-25dp">

        <TextView
            android:id="@+id/text_login_button"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="13.5dp"
            android:layout_marginRight="98dp"
            android:layout_marginBottom="13.5dp"
            android:gravity="center_horizontal|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Log in"
            android:textColor="#FF000000"
            android:textSize="18dp" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_group3"
        android:layout_width="181dp"
        android:layout_height="49dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="104dp"
        android:layout_marginTop="231dp">

        <TextView
            android:id="@+id/text_login"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_gravity="left"
            android:layout_marginTop="-5dp"
            android:layout_marginBottom="-5dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Login"
            android:textColor="#FFED8240"
            android:textSize="22dp" />

        <TextView
            android:id="@+id/text_separator"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_gravity="left"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="-5dp"
            android:layout_marginBottom="-5dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="|"
            android:textColor="#FFFFFFFF"
            android:textSize="22dp" />

        <TextView
            android:id="@+id/text_signup"
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:layout_marginLeft="109dp"
            android:layout_marginTop="-5dp"
            android:layout_marginBottom="-5dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Signup"
            android:textColor="#FFFFFF"
            android:textSize="22dp" />
    </FrameLayout>
</FrameLayout>